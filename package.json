{
  "name": "shape",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.6",
  "description": "A comprehensive graphics algorithm library.",
  "author": "Cheez Lin <mycheez2000@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/cheezone",
  "homepage": "https://github.com/cheezone/Shape#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheezone/Shape.git"
  },
  "bugs": "https://github.com/cheezone/Shape/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint .  --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "type:test": "vitest --typecheck",
    "type:check": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "type-fest": "^4.15.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "bumpp": "^9.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-export-scope": "^2.3.0",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.6",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.8",
    "vitest": "^1.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
